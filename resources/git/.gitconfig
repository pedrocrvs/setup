[alias]
	am = commit --amend --no-edit

	l = log --graph --format=\"%C(brightmagenta)%h %C(brightgreen)%ad %C(white)%s %C(brightblue)<%ae> %C(brightyellow)%d\" --date=format:\"%Y-%m-%d %H:%M\"

[advice]
	addIgnoredFile = false

[core]
	bigFileThreshold = 10m
	editor = code --wait
	eol = lf

[fetch]
  prune = true

[gc]
  auto = 256

[init]
	defaultBranch = main

[pull]
	ff = only
	rebase = true

[push]
	autoSetupRemote = true
	default = simple

[user]
	name = Pedro Cardo Vasconcellos
	email = pedro.cardo.vasconcellos@gmail.com
