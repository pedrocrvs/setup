[
  // remove "workbench.action.closeWindow"
  { "key": "ctrl+shift+w", "command": "-workbench.action.closeWindow" },

  // remove some "workbench.action.terminal" keybindings
  { "key": "ctrl+c", "command": "-workbench.action.terminal.copyAndClearSelection" },
  { "key": "ctrl+shift+c", "command": "-workbench.action.terminal.copySelection" },
  { "key": "ctrl+shift+v", "command": "-workbench.action.terminal.paste" },
  { "key": "ctrl+v", "command": "-workbench.action.terminal.paste" },
  { "key": "ctrl+v", "command": "-workbench.action.terminal.sendSequence" },

  // remove some "workbench.action.quickOpen" keybindings
  { "key": "ctrl+e", "command": "-workbench.action.quickOpen" },
  { "key": "ctrl+e", "command": "-workbench.action.quickOpenNavigateNextInFilePicker" },
  { "key": "ctrl+p", "command": "-workbench.action.quickOpen" },
  { "key": "ctrl+p", "command": "-workbench.action.quickOpenNavigateNextInFilePicker" },
  { "key": "ctrl+q", "command": "-workbench.action.quickOpenNavigateNextInViewPicker" },
  { "key": "ctrl+q", "command": "-workbench.action.quickOpenView" },
  { "key": "ctrl+r", "command": "-workbench.action.quickOpenNavigateNextInRecentFilesPicker" },
  { "key": "ctrl+shift+e", "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker" },
  { "key": "ctrl+shift+p", "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker" },
  { "key": "ctrl+shift+q", "command": "-workbench.action.quickOpenNavigatePreviousInViewPicker" },
  { "key": "ctrl+shift+r", "command": "-workbench.action.quickOpenNavigatePreviousInRecentFilesPicker" },
  { "key": "ctrl+shift+tab", "command": "-workbench.action.quickOpenLeastRecentlyUsedEditorInGroup" },
  { "key": "ctrl+shift+tab", "command": "-workbench.action.quickOpenNavigatePreviousInEditorPicker" },
  { "key": "ctrl+tab", "command": "-workbench.action.quickOpenNavigateNextInEditorPicker" },
  { "key": "ctrl+tab", "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup" },
  { "key": "escape", "command": "-workbench.action.closeQuickOpen" },
  { "key": "shift+escape", "command": "-workbench.action.closeQuickOpen" },

  // remove "markdown.extension" keybindings from yzhang.markdown-all-in-one
  { "key": "alt+c", "command": "-markdown.extension.checkTaskList" },
  { "key": "alt+down", "command": "-markdown.extension.onMoveLineDown" },
  { "key": "alt+s", "command": "-markdown.extension.editing.toggleStrikethrough" },
  { "key": "alt+up", "command": "-markdown.extension.onMoveLineUp" },
  { "key": "backspace", "command": "-markdown.extension.onBackspaceKey" },
  { "key": "ctrl+b", "command": "-markdown.extension.editing.toggleBold" },
  { "key": "ctrl+enter", "command": "-markdown.extension.onCtrlEnterKey" },
  { "key": "ctrl+i", "command": "-markdown.extension.editing.toggleItalic" },
  { "key": "ctrl+k v", "command": "-markdown.extension.closePreview" },
  { "key": "ctrl+m", "command": "-markdown.extension.editing.toggleMath" },
  { "key": "ctrl+oem_4", "command": "-markdown.extension.onOutdentLines" },
  { "key": "ctrl+oem_6", "command": "-markdown.extension.onIndentLines" },
  { "key": "ctrl+shift+oem_4", "command": "-markdown.extension.editing.toggleHeadingDown" },
  { "key": "ctrl+shift+oem_6", "command": "-markdown.extension.editing.toggleHeadingUp" },
  { "key": "ctrl+shift+v", "command": "-markdown.extension.closePreview" },
  { "key": "ctrl+v", "command": "-markdown.extension.editing.paste" },
  { "key": "enter", "command": "-markdown.extension.onEnterKey" },
  { "key": "shift+alt+down", "command": "-markdown.extension.onCopyLineDown" },
  { "key": "shift+alt+up", "command": "-markdown.extension.onCopyLineUp" },
  { "key": "shift+enter", "command": "-markdown.extension.onShiftEnterKey" },
  { "key": "shift+tab", "command": "-markdown.extension.onShiftTabKey" },
  { "key": "tab", "command": "-markdown.extension.onTabKey" },

  // delete item
  { "key": "delete", "command": "-deleteFile" },
  { "key": "shift+delete", "command": "-deleteFile" },
  { "key": "d", "command": "deleteFile", "when": "filesExplorerFocus && !inputFocus" },

  // move cursor
  { "key": "ctrl+down", "command": "cursorMove", "when": "editorTextFocus", "args": { "to": "down", "by": "line", "value": 10 } },
  { "key": "ctrl+up", "command": "cursorMove", "when": "editorTextFocus", "args": { "to": "up", "by": "line", "value": 10 } },
  { "key": "ctrl+shift+down", "command": "cursorMove", "when": "editorTextFocus", "args": { "to": "down", "by": "line", "value": 10, "select": true } },
  { "key": "ctrl+shift+up", "command": "cursorMove", "when": "editorTextFocus", "args": { "to": "up", "by": "line", "value": 10, "select": true } },

  // rename item
  { "key": "f2", "command": "-renameFile" },
  { "key": "r", "command": "renameFile", "when": "filesExplorerFocus && !inputFocus" },

  // editor: delete lines
  { "key": "alt+d", "command": "editor.action.deleteLines", "when": "editorTextFocus && !editorReadonly" },

  // editor: sort lines
  { "key": "alt+a", "command": "editor.action.sortLinesAscending", "when": "editorTextFocus && !editorReadonly" },

  // explorer: copy item
  { "key": "ctrl+c", "command": "-filesExplorer.copy" },
  { "key": "c", "command": "filesExplorer.copy", "when": "filesExplorerFocus && !inputFocus" },

  // explorer: create new item
  { "key": "n", "command": "explorer.newFile", "when": "filesExplorerFocus && !inputFocus" },

  // explorer: cut item
  { "key": "ctrl+x", "command": "-filesExplorer.cut" },
  { "key": "x", "command": "filesExplorer.cut", "when": "filesExplorerFocus && !inputFocus" },

  // explorer: paste item
  { "key": "ctrl+v", "command": "-filesExplorer.paste" },
  { "key": "v", "command": "filesExplorer.paste", "when": "filesExplorerFocus && !inputFocus" },

  // workbench: focus view explorer
  { "key": "ctrl+shift+e", "command": "-workbench.view.explorer", "when": "viewContainer.workbench.view.explorer.enabled" },
  { "key": "ctrl+e", "command": "workbench.view.explorer", "when": "viewContainer.workbench.view.explorer.enabled" },

  // workbench: focus view source control management
  { "key": "ctrl+shift+g", "command": "-workbench.view.scm", "when": "workbench.scm.active" },
  { "key": "ctrl+g", "command": "-workbench.action.gotoLine" },
  { "key": "ctrl+g", "command": "workbench.view.scm", "when": "workbench.scm.active" },

  // workbench: navigate to previous or next recently used editor in group
  { "key": "alt+left", "command": "-workbench.action.navigateBack", "when": "canNavigateBack" },
  { "key": "alt+left", "command": "workbench.action.openPreviousRecentlyUsedEditorInGroup", "when": "editorFocus" },

  { "key": "alt+right", "command": "-workbench.action.navigateForward", "when": "canNavigateForward" },
  { "key": "alt+right", "command": "workbench.action.openNextRecentlyUsedEditorInGroup", "when": "editorFocus" },

  // workbench: open command palette
  { "key": "ctrl+shift+p", "command": "-workbench.action.showCommands" },
  { "key": "ctrl+p", "command": "workbench.action.showCommands" },

  // workbench: open quick open
  { "key": "ctrl+tab", "command": "workbench.action.quickOpen" },
  { "key": "ctrl+tab", "command": "workbench.action.quickOpenNavigateNext", "when": "inQuickOpen" },

  // workbench: scroll through terminal
  { "key": "shift+pagedown", "command": "-workbench.action.terminal.scrollDownPage", "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive" },
  { "key": "shift+down", "command": "workbench.action.terminal.scrollDown", "when": "terminalFocus" },

  { "key": "shift+pageup", "command": "-workbench.action.terminal.scrollUpPage", "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive" },
  { "key": "shift+up", "command": "workbench.action.terminal.scrollUp", "when": "terminalFocus" }
]
